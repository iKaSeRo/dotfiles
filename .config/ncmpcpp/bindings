def_key "="
	volume_up
def_key "+"
    show_clock

# Vim keys
def_key "j"
	scroll_down
def_key "k"
	scroll_up
def_key "h"
	previous_column
def_key "l"
	next_column
def_key "g"
	move_home
def_key "G"
	move_end
def_key "n"
	next_found_item
def_key "N"
	previous_found_item
def_key "."
	show_lyrics
def_key "h"
	jump_to_parent_directory
def_key "l"
	enter_directory

def_key "m"
  show_media_library
def_key "t"
  show_tag_editor
def_key "v"
  show_visualizer
def_key "s"
  reset_search_engine
def_key "s"
  show_search_engine
def_key "x"
  delete_playlist_items
